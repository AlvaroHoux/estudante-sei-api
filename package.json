{
  "name": "estudante-sei-api",
  "version": "1.1.1",
  "description": "A non-official API to interact with UniRV's SEI system, focused on student functionalities.",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./login": {
      "import": "./dist/login.js",
      "types": "./dist/login.d.ts"
    },
    "./materias": {
      "import": "./dist/materias.js",
      "types": "./dist/materias.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "dev": "tsc -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.ts",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --config jest.config.ts",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --config jest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sei",
    "unirv",
    "api",
    "web-scraping",
    "student",
    "student portal"
  ],
  "author": {
    "name": "Alvaro Houx",
    "email": "alvarohoux@gmail.com",
    "url": "https://github.com/AlvaroHoux"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlvaroHoux/estudante-sei-api.git"
  },
  "bugs": {
    "url": "https://github.com/AlvaroHoux/estudante-sei-api/issues"
  },
  "homepage": "https://github.com/AlvaroHoux/estudante-sei-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "css-select": "^5.1.0",
    "dotenv": "^16.4.5",
    "htmlparser2": "^9.1.0"
  }
}
